{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/lib",
    "declarationMap": true,
    "sourceMap": true,
    "declaration": true,
    "inlineSources": true,
    "lib": ["dom", "es2022"]
